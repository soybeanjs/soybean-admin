    "form-data": "^4.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.32.1",
    "pinia": "^2.0.17",
    "qs": "^6.11.0",
    "ua-parser-js": "^1.0.2",
    "vue": "3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.8",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/json": "^2.1.88",
    "@iconify/vue": "^3.2.1",
    "@milahu/patch-package": "^6.4.14",
    "@types/bmapgl": "^0.0.5",
