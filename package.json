    "pinia": "^2.0.23",
    "print-js": "^1.6.0",
    "qs": "^6.11.0",
    "swiper": "^8.4.4",
    "ua-parser-js": "^1.0.32",
    "vditor": "^3.8.18",
    "vue": "3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "wangeditor": "^4.7.15",
    "xgplayer": "^2.32.2"
