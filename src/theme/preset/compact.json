  "sider": {
    "inverted": false,
    "width": 180,
    "collapsedWidth": 48,
    "mixWidth": 80,
    "mixCollapsedWidth": 48,
    "mixChildMenuWidth": 180
  },
  "footer": {
    "visible": false
  }
}
