{
  "name": "Compact Preset",
  "desc": "Compact layout preset for small screens",
  "i18nkey": "theme.appearance.preset.compact",
  "version": "1.0.1",
  "themeRadius": 6,
  "header": {
    "height": 48,
    "breadcrumb": {
      "visible": false
    },
    "multilingual": {
      "visible": false
    },
    "globalSearch": {
      "visible": false
    }
  },
  "tab": {
    "visible": true,
    "cache": true,
    "height": 36,
    "mode": "button",
